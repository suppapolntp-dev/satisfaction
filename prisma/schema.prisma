generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
}

model account {
  user_id       Int    @id
  user_name     String @unique(map: "user_name") @db.VarChar(50)
  user_password String @db.VarChar(255)
  user_role     Int?   @default(1)
  branch_id     Int?
}

model branch {
  branch_id         Int     @id
  brand_id          Int?
  branch_name       String  @db.VarChar(255)
  branch_short_name String? @db.VarChar(10)
  branch_address    String? @db.Text
  email             String? @db.VarChar(255)
  contact           String? @db.VarChar(10)
}

model brand {
  brand_id         Int     @id
  brand_name       String  @db.VarChar(255)
  brand_short_name String? @db.VarChar(10)
  brand_image      String? @db.VarChar(255)
}

model satisfaction {
  satisfaction_id   Int      @id
  satisfaction_name String?  @db.VarChar(25)
  user_id           Int?
  created_at        DateTime @default(now()) @db.Timestamp(0)
}
